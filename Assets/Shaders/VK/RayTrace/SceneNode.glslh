struct GLTFPrimInfo
{
  uint vertexOffset;
  uint indexOffset;
  int materialLayerID;
};

struct Texture 
{
    ivec2 dimensions; // Equivalent to Vector2i in C++
    uint assetID;     // Equivalent to uint32_t in C++
};

struct GLTFMaterialLayer 
{
    int albedoId;
    int bumpId;
    int occlusionId;
    int emissionId;
    int metallicId;
};